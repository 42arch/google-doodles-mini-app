.container {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 0 .8rem;
  color: #323233;
}

.logs {
  display: flex;
  flex-direction: column;
  font-size: .8rem;
}

.logs .log {
  padding: 1rem 0;
  display: flex;
  flex-direction: column;
}

.log .info {
  display: flex;
  width: 50%;
  justify-content: space-between;
}